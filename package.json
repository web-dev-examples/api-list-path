  "description": "List server directory/file via API",
  "main": "index.js",
  "scripts": {
    "nodemon": "nodemon back-end/index.js",
    "start": "node back-end/index.js",
    "ts-build": "npm run ts-build-back-end && npm run ts-build-front-end",
    "ts-build-back-end": "tsc --project tsconfig.back-end.json",
    "ts-build-front-end": "tsc --project tsconfig.front-end.json",
    "ts-watch-back-end": "tsc --project tsconfig.back-end.json ts/back-end --watch",
    "ts-watch-front-end": "tsc --project tsconfig.front-end.json --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/web-dev-examples/api-list-path.git"
