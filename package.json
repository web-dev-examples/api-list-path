  "bugs": {
    "url": "https://github.com/web-dev-examples/api-list-path/issues"
  },
  "homepage": "https://github.com/web-dev-examples/api-list-path#readme",
  "devDependencies": {
    "@types/express": "^4.17.9",
    "@types/node": "^14.14.7",
    "nodemon": "^2.0.6",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.17.1"
  }
